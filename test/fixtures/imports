/*
 * compiled to sass
 */
@import "foo.scss";
@import "foo";

/*
 * compiled to css
 */
@import "foo.css";
@import "foo" screen;
@import "http://foo.com/bar";
@import url(foo);
// variable interpolation
$family: unquote("Droid+Sans");
@import url("http://wat.com/foo?family=#{$family}");

/*
 * multiple files
 */
@import "foo-foo", "foo-foo-foo";

/*
 * nested
 */
#main {
  @import "foo";
}