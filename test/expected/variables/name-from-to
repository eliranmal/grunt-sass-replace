$my-var: "bar";
$my-default-var: "bar" !default;
$my_var: "foo";
$my_default_var: "foo" !default;
$myVar: "foo";
$myDefaultVar: "foo"   !default;
$my-num-var: 10;
$my-num-default-var: 10 !default;
$my_num_var: 6.666;
$my_num_default_var: 3.333 !default;
$myNumVar: 10px;
$myNumDefaultVar: 20em   !default;